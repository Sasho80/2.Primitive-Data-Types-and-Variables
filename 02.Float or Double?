using System;

class DetermineTheType
{
    static void Main()
    {
        float varFloat1 = 12.345f;
        double varDouble1 = 34.567839023;
        float varFloat2 = 3456.091f;
        double varDouble2 = 8923.1234857;
        Console.WriteLine("float={0}", varFloat1);
        Console.WriteLine("double={0}", varDouble1);
        Console.WriteLine("float={0}", varFloat2);
        Console.WriteLine("double={0}", varDouble2);
    }
}
